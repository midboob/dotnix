
{
  "table": {
    "body": [
      "#figure(table(",
      "  columns: ${1:2},",
      "  ${2:// optional: stroke / gutter / fill settings}",
      "  table.header[${3:Header 1}][${4:Header 2}${5:][Header 3]}],",
      "  ${6:// cell‑1‑1}, ${7:// cell‑1‑2}${8:, ${9:// cell‑1‑3}}",
      "  ${10:// cell‑2‑1}, ${11:// cell‑2‑2}${12:, ${13:// cell‑2‑3}}",
      "),",
      "caption: [${14:caption}],",
      "label: <${15:label}>",
      ")"
    ],
    "description": "typst table",
    "prefix": "table"
  },
  "image": {
    "body": [
      "#figure(",
      "  image(\"${1:path/to/image.png}\", width: ${2:50%}${3:, alt: \"${4:description}\"}),",
      "  caption: [${5:caption text}],",
      "  label: <${6:label}>",
      ")",
      "$0"
    ],
    "description": "typst image/figure",
    "prefix": "image",
  },
  "//": {
    "body": [
      "\\frac{${1:numerator}}{${2:denominator}}$0"
    ],
    "description": "math fraction",
    "prefix": "//"
  }
}
